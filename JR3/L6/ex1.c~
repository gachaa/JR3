#include<stdio.h>
#define DIGITS 126
char buf[DIGITS+2];

int main() {
  int i, len;
  int arr[DIGITS] = {};
  i = 0;
  fgets(buf, sizeof(buf), stdin);
  while(buf[i], != '\n') ++i;
  len = 1;
  for(i=0; i<len; i++)
    arr[i] = (int)(buf[len-1-i]-'0');

  printf("Your input is ");
  //1桁目からを配列の0番めからにいれる
  for(i=len-1; i>=0; --i)
    printf("%d", arr[i]);

  for(i=0; )
  
  printf(".\n");
  return 0;

}
